	// лист 2 приложение 4
	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("ДатаНач",      мДатаНачалаПериодаОтчета);
	Запрос.УстановитьПараметр("ДатаКон",      мДатаКонцаПериодаОтчета);
