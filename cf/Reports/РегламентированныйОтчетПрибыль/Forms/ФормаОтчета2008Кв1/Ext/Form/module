	// лист 2 приложение 1
	Счет9001 = ПС.ВыручкаНеЕНВД;
	Счет9101 = ПС.ПрочиеДоходы;
	Счет9002 = ПС.СебестоимостьПродажНеЕНВД;;
	Счет9102 = ПС.ПрочиеРасходы;

	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("СписокОП",  СписокОП);
