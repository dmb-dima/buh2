	Счет77      = ПС.ОтложенныеНалоговыеОбязательства;
	Счет79      = ПС.ВнутрихозяйственныеРасчеты;
	Счет94      = ПС.НедостачиИПотериОтПорчиЦенностей;
	Счет96      = ПС.РезервыПредстоящихРасходов;
	Счет97      = ПС.РасходыБудущихПериодов;
	
	// Проверим есть ли счет 99.02.4 в ПС
	Ссылка = ПС.НайтиПоКоду("99.02.4");
	Если Ссылка.Код <> "" Тогда
		Есть99_02_4 = Истина;
		Счет99_02_4 = ПС.ПрибылиИУбыткиОтПересчетаОНА_ОНО;
	Иначе 
		Есть99_02_4 = Ложь;
		Счет99_02_4 = Неопределено;
	КонецЕсли;


	СписокОрганизаций = Новый СписокЗначений;
	СписокОрганизаций.Добавить(Организация);
	ОбщегоНазначения.ЗагрузитьВТаблицуЗначений(мГруппаОрганизаций, СписокОрганизаций);

	Для каждого Орг Из СписокОрганизаций Цикл

		мОргРасшифровка = Орг.Значение;

		// Бухгалтерский итоги по счетам
		мБухИтоги = Обработки.БухгалтерскиеИтоги.Создать();

		// Бухгалтерские итоги по аналитике
		мБухИтогиРС = Обработки.БухгалтерскиеИтоги.Создать();
		
		Итоги = "НачальныйОстатокДт, НачальныйРазвернутыйОстатокДт, 
		        |НачальныйОстатокКт, НачальныйРазвернутыйОстатокКт,
		        |КонечныйОстатокДт,  КонечныйРазвернутыйОстатокДт, 
		        |КонечныйОстатокКт,  КонечныйРазвернутыйОстатокКт";
		
		// Счета учета расчетов, на которых допустим учет авансов, - для расчета развернутого сальдо
		СчетаУчетаРасчетов = Новый Массив;
		СчетаУчетаРасчетов.Добавить(Счет60_01);
		СчетаУчетаРасчетов.Добавить(Счет60_21);
		СчетаУчетаРасчетов.Добавить(Счет60_31);
		СчетаУчетаРасчетов.Добавить(Счет62_01);
		СчетаУчетаРасчетов.Добавить(Счет62_21);
		СчетаУчетаРасчетов.Добавить(Счет62_31);

		// Виды субконто для расчета развернутого сальдо по счетам учета расчетов
		ВидыСубконтоДляРасчета = Новый Массив;
		ВидыСубконтоДляРасчета.Добавить(ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Контрагенты);
		ВидыСубконтоДляРасчета.Добавить(ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Договоры);

		// Бухгалтерские итоги по счетам, без аналитики
		мБухИтоги.РассчитатьИтоги("Хозрасчетный", Итоги, "Сумма", "Счет", мДатаНачалаПериодаОтчета, мДатаКонцаПериодаОтчета, , , , , , "Организация", мОргРасшифровка);

		// Вспомогательные значения периода
		НачалоПериодаОтчета = мДатаНачалаПериодаОтчета;
		КонецПериодаОтчета  = мДатаКонцаПериодаОтчета;
		
		// строка 1 "Нематериальные активы"
		СНД_04_01 = СНД(Счет04_01);
		СНК_05    = СНК(Счет05);
		СНД_08_05 = СНД(Счет08_05);

		П010011003 = СНД_04_01 - СНК_05 + СНД_08_05;
		УстановитьЗначениеПоказателя("П010011003", П010011003);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010011003", "Строка ""1. Нематериальные активы"", графа ""На начало отчетного года""", "+", "!СНД", СНД_04_01, Счет04_01, , мДатаНачалаПериодаОтчета, мДатаКонцаПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010011003", "", "-", "!СНК", -СНК_05  , Счет05, , мДатаНачалаПериодаОтчета, мДатаКонцаПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010011003", "", "+", "!СНД", СНД_08_05, Счет08_05, , мДатаНачалаПериодаОтчета, мДатаКонцаПериодаОтчета, "ФормаОтчета");


		СКД_04_01 = СКД(Счет04_01);
		СКК_05    = СКК(Счет05);
		СКД_08_05 = СКД(Счет08_05);

		П010011004 = СКД_04_01 - СКК_05 + СКД_08_05;
		УстановитьЗначениеПоказателя("П010011004", П010011004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010011004", "Строка ""1. Нематериальные активы"", графа ""На конец отчетного периода""", "+", "!СКД", СКД_04_01, Счет04_01, , мДатаНачалаПериодаОтчета, мДатаКонцаПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010011004", "", "-", "!СКК", -СКК_05  , Счет05, , мДатаНачалаПериодаОтчета, мДатаКонцаПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010011004", "", "+", "!СКД", СКД_08_05, Счет08_05, , мДатаНачалаПериодаОтчета, мДатаКонцаПериодаОтчета, "ФормаОтчета");
		
		// строка 2 "Основные средства"
		СНД_01_01 = СНД(Счет01_01);
		СНД_01_08 = СНД(Счет01_08);
		СНК_02_01 = СНК(Счет02_01);
		СНД_07 	  = СНД(Счет07);
		СНД_08_01 = СНД(Счет08_01);
		СНД_08_02 = СНД(Счет08_02);
		СНД_08_03 = СНД(Счет08_03);
		СНД_08_04 = СНД(Счет08_04);

		СНК_02_01__01_01 = 0; // Амортизация ОС, учитываемых на счете 01.01
		СНК_02_01__01_08 = 0; // Амортизация ОС, учитываемых на счете 01.08
		// разделим сальдо по счету 02.01 по основным средствам, учитываемым на счетах 01.01 и 01.08
		ИтогиРС = "НачальныйОстатокДт, НачальныйРазвернутыйОстатокДт, 
		          |НачальныйОстатокКт, НачальныйРазвернутыйОстатокКт,
		          |КонечныйОстатокДт,  КонечныйРазвернутыйОстатокДт, 
		          |КонечныйОстатокКт,  КонечныйРазвернутыйОстатокКт";
		мБухИтогиРС.РассчитатьИтоги("Хозрасчетный", ИтогиРС, "Сумма", "Счет, Субконто1", НачалоПериодаОтчета, КонецПериодаОтчета, , "01.01, 01.08, 02.01", ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства, , , "Организация", мОргРасшифровка);
		ОстаткиИОбороты = мБухИтогиРС.ПолучитьОстаткиИОбороты();
		ВыборкаОС = ОстаткиИОбороты.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам, "Субконто1");
		Пока ВыборкаОС.Следующий() Цикл
			// пропускаем группы как элемент иерархии результатов запроса
			Если ВыборкаОС.Субконто1.ЭтоГруппа Тогда
				Продолжить;
			КонецЕсли;
			
			ОС__01_01 = мБухИтогиРС.ПолучитьИтог("СуммаНачальныйОстатокДт", "Счет, Субконто1", Счет01_01, ВыборкаОС.Субконто1);
			ОС__01_08 = мБухИтогиРС.ПолучитьИтог("СуммаНачальныйОстатокДт", "Счет, Субконто1", Счет01_08, ВыборкаОС.Субконто1);
			ОС__02_01 = мБухИтогиРС.ПолучитьИтог("СуммаНачальныйОстатокКт", "Счет, Субконто1", Счет02_01, ВыборкаОС.Субконто1);
			
			Если ОС__01_01 <> 0 Тогда
				СНК_02_01__01_01 = СНК_02_01__01_01 + ОС__02_01;
			ИначеЕсли ОС__01_08 <> 0 Тогда
				СНК_02_01__01_08 = СНК_02_01__01_08 + ОС__02_01;
			Иначе
				// подразумевается что все амортизируемые ОС учитываются либо на счете 01.01, либо на счете 01.08
			КонецЕсли;
			
		КонецЦикла;
		
		П010012003 = СНД_01_01 - СНК_02_01__01_01 
				   + СНД_01_08 - СНК_02_01__01_08
				   + СНД_07 + СНД_08_01 + СНД_08_02 + СНД_08_03 + СНД_08_04;
		УстановитьЗначениеПоказателя("П010012003", П010012003);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "2. Основные средства (01, 02)"", графа ""На начало отчетного года""", "+", "!СНД", СНД_01_01, Счет01_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "-", "Сальдо на начало периода по кредиту счета 02.01 в части ОС в организации", -СНК_02_01__01_01, Счет02_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "+", "!СНД", СНД_01_08, Счет01_08, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "-", "Сальдо на начало периода по кредиту счета 02.01 в части объектов недвижимости на которые не зарегистрированы права собственности", -СНК_02_01__01_08, Счет02_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "+", "!СНД", СНД_07, Счет07, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "+", "!СНД", СНД_08_01, Счет08_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "+", "!СНД", СНД_08_02, Счет08_02, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "+", "!СНД", СНД_08_03, Счет08_03, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012003", "", "+", "!СНД", СНД_08_04, Счет08_04, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		
		СКД_01_01 = СКД(Счет01_01);
		СКД_01_08 = СКД(Счет01_08);
		СКК_02_01 = СКК(Счет02_01);
		СКД_07 	  = СКД(Счет07);
		СКД_08_01 = СКД(Счет08_01);
		СКД_08_02 = СКД(Счет08_02);
		СКД_08_03 = СКД(Счет08_03);
		СКД_08_04 = СКД(Счет08_04);

		СКК_02_01__01_01 = 0; // Амортизация ОС, учитываемых на счете 01.01
		СКК_02_01__01_08 = 0; // Амортизация ОС, учитываемых на счете 01.08
		// разделим сальдо по счету 02.01 по основным средствам, учитываемым на счетах 01.01 и 01.08
		ИтогиРС = "НачальныйОстатокДт, НачальныйРазвернутыйОстатокДт, 
		          |НачальныйОстатокКт, НачальныйРазвернутыйОстатокКт,
		          |КонечныйОстатокДт,  КонечныйРазвернутыйОстатокДт, 
		          |КонечныйОстатокКт,  КонечныйРазвернутыйОстатокКт";
		мБухИтогиРС.РассчитатьИтоги("Хозрасчетный", ИтогиРС, "Сумма", "Счет, Субконто1", НачалоПериодаОтчета, КонецПериодаОтчета, , "01.01, 01.08, 02.01", ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства, , , "Организация", мОргРасшифровка);
		ОстаткиИОбороты = мБухИтогиРС.ПолучитьОстаткиИОбороты();
		ВыборкаОС = ОстаткиИОбороты.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам, "Субконто1");
		Пока ВыборкаОС.Следующий() Цикл
			// пропускаем группы как элемент иерархии результатов запроса
			Если ВыборкаОС.Субконто1.ЭтоГруппа Тогда
				Продолжить;
			КонецЕсли;
			
			ОС__01_01 = мБухИтогиРС.ПолучитьИтог("СуммаКонечныйОстатокДт", "Счет, Субконто1", Счет01_01, ВыборкаОС.Субконто1);
			ОС__01_08 = мБухИтогиРС.ПолучитьИтог("СуммаКонечныйОстатокДт", "Счет, Субконто1", Счет01_08, ВыборкаОС.Субконто1);
			ОС__02_01 = мБухИтогиРС.ПолучитьИтог("СуммаКонечныйОстатокКт", "Счет, Субконто1", Счет02_01, ВыборкаОС.Субконто1);
			
			Если ОС__01_01 <> 0 Тогда
				СКК_02_01__01_01 = СКК_02_01__01_01 + ОС__02_01;
			ИначеЕсли ОС__01_08 <> 0 Тогда
				СКК_02_01__01_08 = СКК_02_01__01_08 + ОС__02_01;
			Иначе
				// подразумевается что все амортизируемые ОС учитываются либо на счете 01.01, либо на счете 01.08
			КонецЕсли;
			
		КонецЦикла;
		
		П010012004 = СКД_01_01 - СКК_02_01__01_01 
				   + СКД_01_08 - СКК_02_01__01_08
				   + СКД_07 + СКД_08_01 + СКД_08_02 + СКД_08_03 + СКД_08_04;
		УстановитьЗначениеПоказателя("П010012004", П010012004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "Строка ""2. Основные средства (01, 02)"", графа ""На конец отчетного периода""", "+", "!СКД", СКД_01_01, Счет01_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "-", "Сальдо на конец периода по кредиту счета 02.01 в части ОС в организации", -СКК_02_01__01_01, Счет02_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "+", "!СКД", СКД_01_08, Счет01_08, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "-", "Сальдо на конец периода по кредиту счета 02.01 в части объектов недвижимости на которые не зарегистрированы права собственности", -СКК_02_01__01_08, Счет02_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "+", "!СКД", СКД_07, Счет07, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "+", "!СКД", СКД_08_01, Счет08_01, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "+", "!СКД", СКД_08_02, Счет08_02, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "+", "!СКД", СКД_08_03, Счет08_03, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010012004", "", "+", "!СКД", СКД_08_04, Счет08_04, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");

		// строка 3 "Незавершенное строительство"
		СНД_08_06 = СНД(Счет08_06);
		СНД_08_07 = СНД(Счет08_07);

		П010013003 = СНД_08_06 + СНД_08_07;
		УстановитьЗначениеПоказателя("П010013003", П010013003);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010013003", "Строка ""3. Незавершенное строительство"", графа ""На начало отчетного года""", "+", "!СНД", СНД_08_06, Счет08_06, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010013003", "", "+", "!СНД", СНД_08_07, Счет08_07, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");

		СКД_08_06 = СКД(Счет08_06);
		СКД_08_07 = СКД(Счет08_07);

		П010013004 = СКД_08_06 + СКД_08_07;
		УстановитьЗначениеПоказателя("П010013004", П010013004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010013004", "Строка ""3. Незавершенное строительство"", графа ""На конец отчетного периода""", "+", "!СКД", СКД_08_06, Счет08_06, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010013004", "", "+", "!СКД", СКД_08_07, Счет08_07, , НачалоПериодаОтчета, КонецПериодаОтчета, "ФормаОтчета");

		// строка 4 "Доходные вложения в материальные ценности"
		П010013503_03    = СНД(Счет03);
		П010013503_02_02 = СНК(Счет02_02);

		П010013503 = П010013503_03 - П010013503_02_02;
		УстановитьЗначениеПоказателя("П010013503", П010013503);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010013503", "Строка ""4. Доходные вложения в материальные ценности"", графа ""На начало отчетного года""", "+", "!СНД", П010013503_03, Счет03);
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П010013503", "", "-", "!СНК", -П010013503_02_02, Счет02_02);

