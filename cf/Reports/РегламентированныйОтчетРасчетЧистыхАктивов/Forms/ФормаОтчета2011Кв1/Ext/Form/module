		СНДР_62_01_КонтрагентыДоговоры = мБухИтогиРС.ПолучитьИтог("СуммаНачальныйРазвернутыйОстатокДт",  "Счет", Счет62_01);
		СНДР_62_21_КонтрагентыДоговоры = мБухИтогиРС.ПолучитьИтог("СуммаНачальныйРазвернутыйОстатокДт",  "Счет", Счет62_21);
		СНДР_62_31_КонтрагентыДоговоры = мБухИтогиРС.ПолучитьИтог("СуммаНачальныйРазвернутыйОстатокДт",  "Счет", Счет62_31);

