					мДатаРождСлиста = стрУказаннаяДата1;
					РегламентированнаяОтчетность.роПроставитьВКвадратыЗначения(ЭтаФорма, "ФизЛица", "ФизЛицДатаРожд", 8, стрУказаннаяДата1);
				КонецЕсли;
			КонецЕсли;
		КонецЕсли;
