		ОкрП000020016004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020016004.Значение);
		ОкрП000020018004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020018004.Значение);
		ОкрП000020020004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020020004.Значение);
		ОкрП000020022004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020022004.Значение);
		ОкрП000020024004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020024004.Значение);
		ОкрП000020026004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020026004.Значение);
		ОкрП000020028004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020028004.Значение);

