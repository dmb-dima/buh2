		                                        + ОкрП000020026004
		                                        + ОкрП000020028004;

		мРаздел2.Области.П000020003004.Значение = мРаздел2.Области.П000020006004.Значение;


		// Строки 290 - 300 по графе 4
		ОкрП000020031004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020031004.Значение);
		ОкрП000020033004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020033004.Значение);
		ОкрП000020034004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020034004.Значение);
