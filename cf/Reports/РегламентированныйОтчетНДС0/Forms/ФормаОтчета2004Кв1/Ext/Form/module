		                                        + ОкрП000020036004
		                                        + ОкрП000020037004
		                                        + ОкрП000020038004
		                                        + ОкрП000020039004
		                                        + ОкрП000020040004;

		мРаздел2.Области.П000020030004.Значение = мРаздел2.Области.П000020032004.Значение;
		
		
		// Строки 010 - 030 по графе 6
		мРаздел2.Области.П000020001006.Значение = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020001004.Значение * 20 / 120);
		мРаздел2.Области.П000020002006.Значение = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020002004.Значение * 18 / 118);
		мРаздел2.Области.П000020003006.Значение = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020003004.Значение * 10 / 110);

