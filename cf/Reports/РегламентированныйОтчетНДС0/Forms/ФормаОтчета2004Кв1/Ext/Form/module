		ОкрП000010027004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010027004.Значение);
		ОкрП000010031004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010031004.Значение);
		ОкрП000010032004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010032004.Значение);
		ОкрП000010033004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010033004.Значение);
		ОкрП000010034004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010034004.Значение);
		ОкрП000010035004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010035004.Значение);
		ОкрП000010036004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010036004.Значение);
		ОкрП000010037004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010037004.Значение);
		ОкрП000010038004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010038004.Значение);
		ОкрП000010039004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010039004.Значение);
		ОкрП000010040004 = ОкруглитьЧислоПоФормату(мРаздел1.Области.П000010040004.Значение);

		мРаздел1.Области.П000010041004.Значение = мРаздел1.Области.П000010017004.Значение
		                                        + ОкрП000010020004
		                                        + ОкрП000010021004
		                                        + ОкрП000010022004
		                                        + ОкрП000010023004
		                                        + ОкрП000010024004
		                                        + ОкрП000010025004
		                                        + ОкрП000010026004
		                                        + ОкрП000010027004
		                                        + мРаздел1.Области.П000010028004.Значение
		                                        + ОкрП000010031004
		                                        + ОкрП000010032004
		                                        + ОкрП000010033004
		                                        + ОкрП000010034004
		                                        + ОкрП000010035004
		                                        + ОкрП000010036004
		                                        + ОкрП000010037004
		                                        + ОкрП000010038004
		                                        + ОкрП000010039004
		                                        - ОкрП000010040004;

	ИначеЕсли ИмяТекущейСтраницыПанели = "Раздел2" Тогда

		// РАЗДЕЛ 2

		// Строки 040 - 060, графа 4
		ОкрП000020007004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020007004.Значение);
		ОкрП000020008004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020008004.Значение);
		ОкрП000020009004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020009004.Значение);
		ОкрП000020010004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020010004.Значение);
		ОкрП000020011004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020011004.Значение);
		ОкрП000020012004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020012004.Значение);

		мРаздел2.Области.П000020004004.Значение = ОкрП000020007004 + ОкрП000020010004;
		мРаздел2.Области.П000020005004.Значение = ОкрП000020008004 + ОкрП000020011004;
		мРаздел2.Области.П000020006004.Значение = ОкрП000020009004 + ОкрП000020012004;

		// Строки 010 - 030, графа 4
		ОкрП000020013004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020013004.Значение);
		ОкрП000020015004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020015004.Значение);
		ОкрП000020017004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020017004.Значение);
		ОкрП000020019004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020019004.Значение);
		ОкрП000020021004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020021004.Значение);
		ОкрП000020023004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020023004.Значение);
		ОкрП000020025004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020025004.Значение);
		ОкрП000020027004 = ОкруглитьЧислоПоФормату(мРаздел2.Области.П000020027004.Значение);

		мРаздел2.Области.П000020001004.Значение = мРаздел2.Области.П000020004004.Значение
		                                        + ОкрП000020013004
