//
// Версия формы
// мВерсияФормы = "02/03/2005";
// мВерсияФормы = "01/05/2005"; исправления в макете
мВерсияФормы = "01/06/2005";

// Код отчета, равен КНД
мКодОтчета   = "1151058";

