//мВерсияФормы = "01/03/2005";
мВерсияФормы = "01/05/2005";

// Код отчета, равен КНД
мКодОтчета   = 1152004;

ОбязРазделов = Новый СписокЗначений;
ОбязРазделов.Добавить("Раздел1");
ОбязРазделов.Добавить("Раздел2");
ОбязРазделов.Добавить("Раздел3");

