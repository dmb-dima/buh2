		мДатаНачалаПериодаПрошлогоГода = НачалоГода(мДатаКонцаПериодаПрошлогоГода);

		Итоги = "НачальныйОстатокДт, НачальныйОстатокКт, КонечныйОстатокДт, КонечныйОстатокКт";

		// Бухгалтерские итоги по счетам, без аналитики
		//мБухИтоги.РассчитатьИтоги("Хозрасчетный", Итоги, "Сумма", "Счет", мДатаНачалаПериодаПрошлогоГода, мДатаКонцаПериодаПрошлогоГода, , , , , , "Организация", мОргРасшифровка);
		мБухИтоги.РассчитатьИтоги("Хозрасчетный", Итоги, "Сумма", "Счет", мДатаНачалаПериодаПрошлогоГода, мДатаКонцаПериодаПрошлогоГода, , "86", ВидыСубконто, , , СтруктураОтбора);
		
		// строка 100 "Остаток средств на начало отчетного года"
		П010010004_86_К = СНК(Счет86);
		П010010004_86_Д = СНД(Счет86);

		П010010004 = П010010004_86_К - П010010004_86_Д;
		УстановитьЗначениеПоказателя("П000010010004", П010010004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010010004","Строка 100 ""Остаток средств на начало отчетного года"", графа 4 ""За предыдущий год""","+","!СНК", П010010004_86_К, Счет86, , мДатаНачалаПериодаПрошлогоГода, мДатаКонцаПериодаПрошлогоГода);
		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010010004","","-","!СНД", -П010010004_86_Д, Счет86, , мДатаНачалаПериодаПрошлогоГода, мДатаКонцаПериодаПрошлогоГода);
		
		Итоги = "ОборотДт, ОборотКт";
		//мБухИтогиРС.РассчитатьИтоги("Хозрасчетный", Итоги, "Сумма", "Счет, Субконто1", мДатаНачалаПериодаПрошлогоГода, мДатаКонцаПериодаПрошлогоГода, , "86", ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ДвиженияЦелевыхСредств, , , "Организация", мОргРасшифровка);
		мБухИтогиРС.РассчитатьИтоги("Хозрасчетный", Итоги, "Сумма", "Счет, Субконто1, Субконто2", мДатаНачалаПериодаПрошлогоГода, мДатаКонцаПериодаПрошлогоГода, , "86", ВидыСубконто, , , СтруктураОтбора);
		РезультатЗапроса = мБухИтогиРС.ПолучитьОстаткиИОбороты();

		// Начальное обнуление
		П010021004_ВступительныеВзносы = 0;
		П010022004_ЧленскиеВзносы = 0;
		П010023004_ДобровольныеВзносы = 0;
		П010024004_ДоходыОтПредпринимательскойДеятельностиОрганизации = 0;
		П010025004_ПрочиеПоступления = 0;
		Если ЭтоБалансДляАУ Тогда
			П010025104_ПоступленияНаОказаниеГосударственныхМуниципальныхУслуг = 0;
		КонецЕсли;
		
		П010031104_ОказаниеСоциальнойИБлаготворительнойПомощи = 0;
		П010031204_ПроведениеКонференцийСеминаров = 0;
		П010031304_ПроведениеИныхЦелевыхМероприятий = 0;
		
		П010032104_РасходыСвязанныеСОплатойТруда = 0;
		П010032204_ВыплатыНеСвязанныеСОплатойТруда = 0;
		П010032304_РасходыНаСлужебныеКомандировкиИДеловыеПоездки = 0;
		П010032404_СодержаниеПомещенийЗданийАвтотранспортаИИногоИмущества = 0;
		П010032504_РемонтОсновныхСредствИИногоИмущества = 0;
		П010032604_ПрочиеРасходыНаСодержаниеАппаратаУправления = 0;
		
		П010033004_ПриобретениеОсновныхСредствИнвентаряИИногоИмущества = 0;
		П010034004_РасходыСвязанныеСПредпринимательскойДеятельностью = 0;
		П010035004_ПрочиеРасходы = 0;
		Если ЭтоБалансДляАУ Тогда
			П010035104_ОказаниеГосударственныхМуниципальныхУслуг = 0;
		КонецЕсли;
		
		// Обрабатываем субкотно "Движения целевых средств".
		ВыборкаПоИсточникамЦС = РезультатЗапроса.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам, "Субконто1");
		Пока ВыборкаПоИсточникамЦС.Следующий() Цикл

			ВыбранныйИсточникЦС = ВыборкаПоИсточникамЦС.Субконто1;

			Если ВыбранныйИсточникЦС.ЭтоГруппа Тогда
				Продолжить;
			КонецЕсли;

			// Сразу получем все необходимые итоги
			Сумма86_КО = мБухИтогиРС.ПолучитьИтог("СуммаОборотКт", "Счет, Субконто1", Счет86, ВыбранныйИсточникЦС);
			
			Если ВыбранныйИсточникЦС.ВидИсточника = Перечисления.ВидыИсточниковЦелевыхСредств.ВступительныеВзносы Тогда

				П010021004_ВступительныеВзносы = П010021004_ВступительныеВзносы + Сумма86_КО;
				
			ИначеЕсли ВыбранныйИсточникЦС.ВидИсточника = Перечисления.ВидыИсточниковЦелевыхСредств.ЧленскиеВзносы Тогда

				П010022004_ЧленскиеВзносы = П010022004_ЧленскиеВзносы + Сумма86_КО;
				
			ИначеЕсли ВыбранныйИсточникЦС.ВидИсточника = Перечисления.ВидыИсточниковЦелевыхСредств.ДобровольныеВзносы Тогда

				П010023004_ДобровольныеВзносы = П010023004_ДобровольныеВзносы + Сумма86_КО;
				
			ИначеЕсли ВыбранныйИсточникЦС.ВидИсточника = Перечисления.ВидыИсточниковЦелевыхСредств.ДоходыОтПредпринимательскойДеятельностиОрганизации Тогда

				П010024004_ДоходыОтПредпринимательскойДеятельностиОрганизации = П010024004_ДоходыОтПредпринимательскойДеятельностиОрганизации + Сумма86_КО;
				
			Иначе
				
				П010025004_ПрочиеПоступления	= П010025004_ПрочиеПоступления + Сумма86_КО;
				
				Если ЭтоБалансДляАУ И ВыбранныйИсточникЦС.ВидИсточника = Перечисления.ВидыИсточниковЦелевыхСредств.ПоступленияНаОказаниеГосударственныхМуниципальныхУслуг Тогда
					П010025104_ПоступленияНаОказаниеГосударственныхМуниципальныхУслуг = П010025104_ПоступленияНаОказаниеГосударственныхМуниципальныхУслуг + Сумма86_КО;
				КонецЕсли;
				
			КонецЕсли;
			
			// Обрабатываем субкотно "Статьи использования целевых средств".
			ВыборкаПоИсточникамИСтатьям	= ВыборкаПоИсточникамЦС.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам, "Субконто2");
			Пока ВыборкаПоИсточникамИСтатьям.Следующий() Цикл
					
				СтатьяРасходовЦелевыхСредств = ВыборкаПоИсточникамИСтатьям.Субконто2;
				
				Если СтатьяРасходовЦелевыхСредств.ЭтоГруппа Тогда
					Продолжить;
				КонецЕсли;
				
				// Сразу получем все необходимые итоги
				Сумма86_ДО = мБухИтогиРС.ПолучитьИтог("СуммаОборотДт", "Счет, Субконто1, Субконто2", Счет86, ВыбранныйИсточникЦС, СтатьяРасходовЦелевыхСредств);

				Если СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.ОказаниеСоциальнойИБлаготворительнойПомощи Тогда

					П010031104_ОказаниеСоциальнойИБлаготворительнойПомощи = П010031104_ОказаниеСоциальнойИБлаготворительнойПомощи + Сумма86_ДО;
					
				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.ПроведениеКонференцийСеминаров Тогда

					П010031204_ПроведениеКонференцийСеминаров = П010031204_ПроведениеКонференцийСеминаров + Сумма86_ДО;
					
				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.ПроведениеИныхЦелевыхМероприятий Тогда

					П010031304_ПроведениеИныхЦелевыхМероприятий = П010031304_ПроведениеИныхЦелевыхМероприятий + Сумма86_ДО;
					
				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.РасходыСвязанныеСОплатойТруда Тогда
					
					П010032104_РасходыСвязанныеСОплатойТруда = П010032104_РасходыСвязанныеСОплатойТруда + Сумма86_ДО;

				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.ВыплатыНеСвязанныеСОплатойТруда Тогда

					П010032204_ВыплатыНеСвязанныеСОплатойТруда = П010032204_ВыплатыНеСвязанныеСОплатойТруда + Сумма86_ДО;

				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.РасходыНаСлужебныеКомандировкиИДеловыеПоездки Тогда

					П010032304_РасходыНаСлужебныеКомандировкиИДеловыеПоездки = П010032304_РасходыНаСлужебныеКомандировкиИДеловыеПоездки + Сумма86_ДО;

				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.СодержаниеПомещенийЗданийАвтотранспортаИИногоИмущества Тогда
					
					П010032404_СодержаниеПомещенийЗданийАвтотранспортаИИногоИмущества = П010032404_СодержаниеПомещенийЗданийАвтотранспортаИИногоИмущества + Сумма86_ДО;
					
				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.РемонтОсновныхСредствИИногоИмущества Тогда
					
					П010032504_РемонтОсновныхСредствИИногоИмущества = П010032504_РемонтОсновныхСредствИИногоИмущества + Сумма86_ДО;
					
				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.ПрочиеРасходыНаСодержаниеАппаратаУправления Тогда

					П010032604_ПрочиеРасходыНаСодержаниеАппаратаУправления = П010032604_ПрочиеРасходыНаСодержаниеАппаратаУправления + Сумма86_ДО;
					
				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.ПриобретениеОсновныхСредствИнвентаряИИногоИмущества Тогда

					П010033004_ПриобретениеОсновныхСредствИнвентаряИИногоИмущества = П010033004_ПриобретениеОсновныхСредствИнвентаряИИногоИмущества + Сумма86_ДО;
					
				ИначеЕсли СтатьяРасходовЦелевыхСредств.ВидРасходовЦелевыхСредств = Перечисления.ВидыРасходовЦелевыхСредств.РасходыСвязанныеСПредпринимательскойДеятельностью Тогда

					П010034004_РасходыСвязанныеСПредпринимательскойДеятельностью = П010034004_РасходыСвязанныеСПредпринимательскойДеятельностью + Сумма86_ДО;
					
				Иначе

					П010035004_ПрочиеРасходы		= П010035004_ПрочиеРасходы + Сумма86_ДО;
					
				КонецЕсли;
				
			КонецЦикла;
			
		КонецЦикла;
		
		// строка 210 "Вступительные взносы"
		П010021004 = П010021004_ВступительныеВзносы;
		УстановитьЗначениеПоказателя("П000010021004", П010021004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010021004", "Строка 210 ""Вступительные взносы"", графа 4 ""За предыдущий год""", "+", "Оборот по кредиту счета 86 /статья движения целевых средств - ""Вступительные взносы""/",  П010021004_ВступительныеВзносы);
		
		// строка 220 "Членские взносы"
		П010022004 = П010022004_ЧленскиеВзносы;
		УстановитьЗначениеПоказателя("П000010022004", П010022004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010022004", "Строка 220 ""Членские взносы"", графа 4 ""За предыдущий год""", "+", "Оборот по кредиту счета 86 /статья движения целевых средств - ""Членские взносы""/",  П010022004_ЧленскиеВзносы);
		
		// строка 230 "Доброволные взносы"
		П010023004 = П010023004_ДобровольныеВзносы;
		УстановитьЗначениеПоказателя("П000010023004", П010023004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010023004", "Строка 230 ""Доброволные взносы"", графа 4 ""За предыдущий год""", "+", "Оборот по кредиту счета 86 /статья движения целевых средств - ""Доброволные взносы""/",  П010023004_ДобровольныеВзносы);
		
		// строка 240 "Доходы от предпринимательской деятельности организации"
		П010024004 = П010024004_ДоходыОтПредпринимательскойДеятельностиОрганизации;
		УстановитьЗначениеПоказателя("П000010024004", П010024004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010024004", "Строка 240 ""Доходы от предпринимательской деятельности организации"", графа 4 ""За предыдущий год""", "+", "Оборот по кредиту счета 86 /статья движения целевых средств - ""Доходы от предпринимательской деятельности организации""/",  П010024004_ДоходыОтПредпринимательскойДеятельностиОрганизации);
		
		// строка 250 "Прочие поступления"
		П010025004 = П010025004_ПрочиеПоступления;
		УстановитьЗначениеПоказателя("П000010025004", П010025004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010025004", "Строка 250 ""Прочие поступления"", графа 4 ""За предыдущий год""", "+", "Оборот по кредиту счета 86 /статья движения целевых средств - ""Прочие поступления""/",  П010025004_ПрочиеПоступления);
		
		Если ЭтоБалансДляАУ Тогда
			// строка 251 "Поступления на оказание государственных (муниципальных) услуг"
			П010025104 = П010025104_ПоступленияНаОказаниеГосударственныхМуниципальныхУслуг;
			УстановитьЗначениеПоказателя("П000010025104", П010025104);

			РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010025104", "Строка 251 ""Поступления на оказание государственных (муниципальных) услуг"", графа 4 ""За предыдущий год""", "+", "Оборот по кредиту счета 86 /статья движения целевых средств - ""Поступления на оказание государственных (муниципальных) услуг""/",  П010025104_ПоступленияНаОказаниеГосударственныхМуниципальныхУслуг);
		КонецЕсли;
		
		// строка 311 "Оказание социальной и благотворительной помощи"
		П010031104 = П010031104_ОказаниеСоциальнойИБлаготворительнойПомощи;
		УстановитьЗначениеПоказателя("П000010031104", -П010031104);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010031104", "Строка 311 ""Оказание социальной и благотворительной помощи"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Оказание социальной и благотворительной помощи""/",  П010031104_ОказаниеСоциальнойИБлаготворительнойПомощи);
		
		// строка 312 "Проведение конференций семинаров и т.п."
		П010031204 = П010031204_ПроведениеКонференцийСеминаров;
		УстановитьЗначениеПоказателя("П000010031204", -П010031204);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010031204", "Строка 312 ""Проведение конференций семинаров и т.п."", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Проведение конференций семинаров и т.п.""/",  П010031204_ПроведениеКонференцийСеминаров);
		
		// строка 313 "Проведение конференций семинаров и т.п."
		П010031304 = П010031304_ПроведениеИныхЦелевыхМероприятий;
		УстановитьЗначениеПоказателя("П000010031304", -П010031304);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010031304", "Строка 313 ""Проведение иных целевых мероприятий"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Проведение иных целевых мероприятий""/",  П010031304_ПроведениеИныхЦелевыхМероприятий);
		
		// строка 321 "Расходы, связанные с оплатой труда (включая начисления)"
		П010032104 = П010032104_РасходыСвязанныеСОплатойТруда;
		УстановитьЗначениеПоказателя("П000010032104", -П010032104);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010032104", "Строка 321 ""Расходы, связанные с оплатой труда (включая начисления)"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Расходы, связанные с оплатой труда""/",  П010032104_РасходыСвязанныеСОплатойТруда);
		
		// строка 322 "Выплаты, не связанные с оплатой труда"
		П010032204 = П010032204_ВыплатыНеСвязанныеСОплатойТруда;
		УстановитьЗначениеПоказателя("П000010032204", -П010032204);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010032204", "Строка 322 ""Выплаты, не связанные с оплатой труда"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Выплаты, не связанные с оплатой труда""/",  П010032204_ВыплатыНеСвязанныеСОплатойТруда);
		
		// строка 323 "Расходы на служебные коммандировки и деловые поездки"
		П010032304 = П010032304_РасходыНаСлужебныеКомандировкиИДеловыеПоездки;
		УстановитьЗначениеПоказателя("П000010032304", -П010032304);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010032304", "Строка 323 ""Расходы на служебные коммандировки и деловые поездки"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Расходы на служебные коммандировки и деловые поездки""/",  П010032304_РасходыНаСлужебныеКомандировкиИДеловыеПоездки);
		
		// строка 324 "Содержание помещений, зданий, автомобильного транспорта и иного имущества"
		П010032404 = П010032404_СодержаниеПомещенийЗданийАвтотранспортаИИногоИмущества;
		УстановитьЗначениеПоказателя("П000010032404", -П010032404);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010032404", "Строка 324 ""Содержание помещений, зданий, автомобильного транспорта и иного имущества"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Содержание помещений, зданий, автомобильного транспорта и иного имущества""/",  П010032404_СодержаниеПомещенийЗданийАвтотранспортаИИногоИмущества);
		
		// строка 325 "Ремонт основных средств и иного имущества"
		П010032504 = П010032504_РемонтОсновныхСредствИИногоИмущества;
		УстановитьЗначениеПоказателя("П000010032504", -П010032504);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010032504", "Строка 325 ""Ремонт основных средств и иного имущества"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Ремонт основных средств и иного имущества""/",  П010032504_РемонтОсновныхСредствИИногоИмущества);
		
		// строка 326 "Прочие расходы на содержание аппарата управления"
		П010032604 = П010032604_ПрочиеРасходыНаСодержаниеАппаратаУправления;
		УстановитьЗначениеПоказателя("П000010032604", -П010032604);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010032604", "Строка 326 ""Прочие расходы на содержание аппарата управления"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Прочие расходы на содержание аппарата управления""/",  П010032604_ПрочиеРасходыНаСодержаниеАппаратаУправления);
		
		// строка 330 "Приобретение основных средств, инвентаря и иного имущества"
		П010033004 = П010033004_ПриобретениеОсновныхСредствИнвентаряИИногоИмущества;
		УстановитьЗначениеПоказателя("П000010033004", -П010033004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010033004", "Строка 330 ""Приобретение основных средств, инвентаря и иного имущества"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Приобретение основных средств, инвентаря и иного имущества""/",  П010033004_ПриобретениеОсновныхСредствИнвентаряИИногоИмущества);
		
		// строка 340 "Расходы связанные с предпринимательской деятельностью"
		П010034004 = П010034004_РасходыСвязанныеСПредпринимательскойДеятельностью;
		УстановитьЗначениеПоказателя("П000010034004", -П010034004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010034004", "Строка 340 ""Расходы связанные с предпринимательской деятельностью"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Расходы связанные с предпринимательской деятельностью""/",  П010034004_РасходыСвязанныеСПредпринимательскойДеятельностью);
		
		// строка 350 "Прочие расходы"
		П010035004 = П010035004_ПрочиеРасходы;
		УстановитьЗначениеПоказателя("П000010035004", -П010035004);

		РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010035004", "Строка 350 ""Прочие расходы"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 /статья движения целевых средств - ""Прочие расходы""/",  П010035004_ПрочиеРасходы);
		
		Если ЭтоБалансДляАУ Тогда
			// строка 351 "Оказание государственных (муниципальных) услуг"
			П010035104_ОказаниеГосударственныхМуниципальныхУслуг = мБухИтогиРС.Оборот(РегистрыБухгалтерии.Хозрасчетный, "Сумма", "Дт", мДатаНачалаПериодаПрошлогоГода, мДатаКонцаПериодаПрошлогоГода, "86", , "62");
			
			П010035104 = П010035104_ОказаниеГосударственныхМуниципальныхУслуг;
			УстановитьЗначениеПоказателя("П000010035104", -П010035104);

			РегламентированнаяОтчетность._ДобавитьСтрокуРасшифровки(ЭтаФорма, "П000010035104", "Строка 351 ""Оказание государственных (муниципальных) услуг"", графа 4 ""За предыдущий год""", "+", "Оборот по дебету счета 86 / в корреспонденции со счетом 62",  П010035104_ОказаниеГосударственныхМуниципальныхУслуг);
		КонецЕсли;
		
	КонецЦикла;
	
	// В случае, если отчет был консолидирован, то сложим все его показатели в таблице, а затем выведем их на лист.
	Если НЕ мБезКонсолидации Тогда
		мТаблицаИтоговыхПоказателейПриКонсолидации.Свернуть("ИмяПоказателя", "Сумма");
		Для каждого Пок Из мТаблицаИтоговыхПоказателейПриКонсолидации Цикл
			УстановитьЗначениеПоказателя(Пок.ИмяПоказателя, Пок.Сумма, Истина);
		КонецЦикла;
	КонецЕсли;

	Расчет();
	Модифицированность = Истина;

	РегламентированнаяОтчетность.ОтключитьОбработчикАвтосохранения(ЭтаФорма);
	РегламентированнаяОтчетность.ВключитьОбработчикАвтосохранения(ЭтаФорма);

КонецПроцедуры

Процедура КоманднаяПанельФормыЗаполнить(Кнопка)
	
	ЗаполнитьАвто();

КонецПроцедуры

Процедура КоманднаяПанельФормыРасшифровка(Кнопка)
	
	Обраб = Обработки.РасшифровкаРегламентированнойОтчетности.Создать();
	Обраб.Форма = ЭтаФорма;
	Обраб.ИмяЯчейки = ЭлементыФормы.ПолеТабличногоДокументаФормаОтчета.ТекущаяОбласть.Имя;
	Обраб.ПолучитьФорму(, ЭтаФорма).Показать();
	
КонецПроцедуры

Процедура КоманднаяПанельФормыСохранять(Кнопка)
	Кнопка.Пометка = НЕ Кнопка.Пометка;
	мСохранятьРасшифровку = Кнопка.Пометка;
КонецПроцедуры

Процедура Надпись30Нажатие(Элемент)
	
	СписокОрганизаций = Новый СписокЗначений;
	СписокОрганизаций.Добавить(Организация);
	ОбщегоНазначения.ЗагрузитьВТаблицуЗначений(мГруппаОрганизаций, СписокОрганизаций);
	СписокОрганизаций.ВыбратьЭлемент("Список организаций, по которым формируется отчет");
	
КонецПроцедуры

Функция ПолучитьНазванияОрганизацийСтрокой()
	Результат = Организация.Наименование;
	Если мБезКонсолидации Тогда
		Возврат Результат;
	Иначе
		Для каждого Зап Из мГруппаОрганизаций Цикл
			Результат = Результат + "; " + Зап.Значение.Наименование;
		КонецЦикла;
		Возврат Результат;
	КонецЕсли;
КонецФункции

Процедура УстановитьЗначениеПоказателя(ИмяПоказателя, Знач ЗначениеПоказателя, ЗаписьВОтчет = Ложь)

	// В случае, отчет заполняется с использованием режима консолидации, тогда анализируем флаг ЗаписьВОтчет и мБезКонсолидации.
	Если (НЕ ЗаписьВОтчет) И (НЕ мБезКонсолидации) Тогда
		// Добавим показатель в таблицу итогового расчета при заполнении отчета за несколько организаций.
		НовСтр = мТаблицаИтоговыхПоказателейПриКонсолидации.Добавить();
		НовСтр.ИмяПоказателя = ИмяПоказателя;
		НовСтр.Сумма = ЗначениеПоказателя;

		Возврат;

	КонецЕсли;

	ТаблицаВариантыЗаполнения = ПолучитьТаблицуВариантовЗаполнения("ФормаОтчета");
	Если НЕ ТаблицаВариантыЗаполнения = Неопределено Тогда

		НайденнаяСтрока = ТаблицаВариантыЗаполнения.Найти(ИмяПоказателя, "КодПоказателя");
		Если НайденнаяСтрока <> Неопределено Тогда

			ВариантЗаполнения = НайденнаяСтрока.ВариантЗаполнения;

			// В случае, если предлагаемое значение не числовое и второй тип (с корректировкой)
			// тогда принимаем предложенное значение за 0.
			Если (НЕ РегламентированнаяОтчетность.роЯвляетсяЧислом(ЗначениеПоказателя)) И (ВариантЗаполнения = 2) Тогда
				ЗначениеПоказателя = 0;
			КонецЕсли;

			Если РегламентированнаяОтчетность.роЯвляетсяЧислом(ЗначениеПоказателя) Тогда
				НайденнаяСтрока.ЗначениеАвто = ЗначениеПоказателя;
			КонецЕсли;

			Если ВариантЗаполнения = 1 Тогда
				// заполняется только вручную
				Возврат;
			ИначеЕсли ВариантЗаполнения = 2 Тогда
				// автоматически, с возможностью ручной корректировки
				Дельта             = НайденнаяСтрока.Дельта;
				ЗначениеПоказателя = ЗначениеПоказателя + Дельта;
			КонецЕсли;

		КонецЕсли;
	КонецЕсли;

	мФормаОтчета.Области[ИмяПоказателя].Значение = ЗначениеПоказателя;

КонецПроцедуры // УстановитьЗначениеПоказателя()

// Функция вычисляет начальный остаток по кредиту счета, указанного в качестве 
// параметра функции. Бухгалтерские итоги должны быть предварительно рассчитаны
// процедурой РассчитатьИтоги() обработки БухгалтерскиеИтоги.
//
// Параметры:
//  Счет         - значение типа ПланСчетов - счет, для которого требуется
//                 получить итог.
//
// Возвращаемое значение:
//  Число        - начальный остаток по кредиту счета, указанного в параметре.
//
Функция СНК(Счет)

	Перем Итог;

	Возврат мБухИтоги.ПолучитьИтог("СуммаНачальныйОстатокКт", "Счет", Счет);

	Возврат Итог;

КонецФункции // СНК()

// Функция вычисляет начальный остаток по дебету счета, указанного в качестве 
// параметра функции. Бухгалтерские итоги должны быть предварительно рассчитаны
// процедурой РассчитатьИтоги() обработки БухгалтерскиеИтоги.
//
// Параметры:
//  Счет         - значение типа ПланСчетов - счет, для которого требуется
//                 получить итог.
//
// Возвращаемое значение:
//  Число        - начальный остаток по дебету счета, указанного в параметре.
//
Функция СНД(Счет)

	Перем Итог;

	Итог = мБухИтоги.ПолучитьИтог("СуммаНачальныйОстатокДт", "Счет", Счет);

	Возврат Итог;

КонецФункции // СНД()

// Процедура вызывается при выборе пункта "Показать машиночитаемый бланк" подменю "Печать"
// командной панели действий формы. 
//
// Обработчик уставливается динамически методом "Действие" из общего модуля
// "Регламентированная отчетность".
//
Процедура КоманднаяПанельДействийПечатьМашиночитаемогоБланка(Кнопка)
	
	РегламентированнаяОтчетность.ВывестиМашиночитаемуюФорму(ЭтаФорма, Кнопка.Имя);
	
КонецПроцедуры

////////////////////////////////////////////////////////////////////////////////

Процедура КоманднаяПанельФормыДействиеОтправка(Кнопка)
	
	РегламентированнаяОтчетность.ПриНажатииНаКнопкуОтправкиВРегламентированномОтчете(ЭтаФорма, Кнопка);
	
КонецПроцедуры

Процедура СтатусНачалоВыбора(Элемент, СтандартнаяОбработка)
	
	РегламентированнаяОтчетность.НачалоВыбораСтатусаВРегламентированномОтчете(ЭтаФорма, Элемент, СтандартнаяОбработка);
	
КонецПроцедуры

Процедура СтатусПриИзменении(Элемент)
	
	РегламентированнаяОтчетность.ПриИзмененииСтатусаВРегламентированномОтчете(ЭтаФорма, Элемент);
	
КонецПроцедуры

Процедура СтатусОчистка(Элемент, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
	
КонецПроцедуры

Процедура ОбработкаОповещения(ИмяСобытия, Параметр, Источник)
	
	РегламентированнаяОтчетность.ОбработкаОповещенияОбИзмененииСтатусаРегламентированногоОтчета(ЭтаФорма, ИмяСобытия, Параметр, Источник);
	
КонецПроцедуры

////////////////////////////////////////////////////////////////////////////////
// ОПЕРАТОРЫ ОСНОВНОЙ ПРОГРАММЫ

// Список выбора поля выбора "Единица измерения"
ЭлементыФормы.ПолеВыбораЕдиницаИзмерения.СписокВыбора.Добавить(Перечисления.ПорядкиОкругленияОтчетности.Окр1,       "в рублях");
ЭлементыФормы.ПолеВыбораЕдиницаИзмерения.СписокВыбора.Добавить(Перечисления.ПорядкиОкругленияОтчетности.Окр1000,    "в тысячах рублей");
ЭлементыФормы.ПолеВыбораЕдиницаИзмерения.СписокВыбора.Добавить(Перечисления.ПорядкиОкругленияОтчетности.Окр1000000, "в миллионах рублей");

ОписаниеТиповЧисло = ОбщегоНазначения.ПолучитьОписаниеТиповЧисла(15,2);

ОписаниеТиповСтрока = ОбщегоНазначения.ПолучитьОписаниеТиповСтроки(254);

МассивТипов = Новый Массив;
МассивТипов.Добавить(Тип("Дата"));
ОписаниеТиповДата = Новый ОписаниеТипов(МассивТипов, , Новый КвалификаторыДаты(ЧастиДаты.Дата));

ЦветЗаполняетсяТолькоВручную    = Новый Цвет(255, 255, 225);
ЦветЗаполняетсяАвтоКорректир    = Новый Цвет(255, 240, 200);
ЦветЗаполняетсяАвтоБезКорректир = Новый Цвет(230, 240, 220);

// структура цветов раскраски автозаполняемых ячеек
мСтруктураЦветовРаскраски = Новый Структура;
мСтруктураЦветовРаскраски.Вставить("_1", ЦветЗаполняетсяТолькоВручную);
мСтруктураЦветовРаскраски.Вставить("_2", ЦветЗаполняетсяАвтоКорректир);
мСтруктураЦветовРаскраски.Вставить("_3", ЦветЗаполняетсяАвтоБезКорректир);

// ОПИСАНИЕ ПАРМЕТРОВ ФОРМЫ
//
// Версия формы
мВерсияФормы = "30/09/2003";
// Код отчета, равен коду по ОКУД
мКодОтчета   = "0710006";

ОбязРазделов = Новый СписокЗначений;

ИДКонфигурации = РегламентированнаяОтчетность.ИДКонфигурации();
ЭтоБалансДляАУ = (ИДКонфигурации = "БАУКОРП" ИЛИ ИДКонфигурации = "БАУ");

Если ЭтоБалансДляАУ Тогда
	ИмяМакетаССоставомПоказателей = "СоставПоказателейАУ";
Иначе
	ИмяМакетаССоставомПоказателей = "СоставПоказателей";
КонецЕсли;

ВариантыФорматовВыгрузки = Новый СписокЗначений;
ВариантыФорматовВыгрузки.Добавить(ИмяМакетаССоставомПоказателей, "Формат 2.01002", Истина);

ТаблицаСообщений = Новый ТаблицаЗначений;
ТаблицаСообщений.Колонки.Добавить("Раздел");
ТаблицаСообщений.Колонки.Добавить("Графа");
ТаблицаСообщений.Колонки.Добавить("Строка");
ТаблицаСообщений.Колонки.Добавить("Страница");
ТаблицаСообщений.Колонки.Добавить("СтрокаПП");
ТаблицаСообщений.Колонки.Добавить("ИмяЯчейки");
ТаблицаСообщений.Колонки.Добавить("Описание");

