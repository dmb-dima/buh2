ТаблицаСтраницыРазделов_1_2.Колонки.Добавить("ДанныеОбИсточнике");
ТаблицаСтраницыРазделов_1_2.Колонки.Добавить("СведенияОДоходах");
ТаблицаСтраницыРазделов_1_2.Колонки.Добавить("АвтоматическоеПредставление", ОписаниеТиповБулево);

ТаблицаСтраницыРазделов1и2 = ТаблицаСтраницыРазделов_1_2.Скопировать();
мСтруктураРаздел1_2 = Новый Структура;
мСтруктураРаздел1_2.Вставить("Раздел1_2",ТаблицаСтраницыРазделов1и2);

// Структура содержит данные дополнительных страниц раздела 1
мСтруктураМногостраничныхРазделов = Новый Структура;

// Элемент структуры - данные дополнительных страниц ФизЛица
