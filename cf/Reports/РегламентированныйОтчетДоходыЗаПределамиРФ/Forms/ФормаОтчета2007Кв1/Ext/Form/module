СтруктураГруппы2.Колонки.Добавить("П000020010002", ОписаниеТиповСтрока254);
СтруктураГруппы2.Колонки.Добавить("П000020010003", ОбщегоНазначения.ПолучитьОписаниеТиповСтроки(3));
СтруктураГруппы2.Колонки.Добавить("П000020010004", ОписаниеТиповЧисло17_2);
СтруктураГруппы2.Колонки.Добавить("П000020010008", ОписаниеТиповЧисло17_2);
СтруктураГруппы2.Колонки.Добавить("П000020010009", ОписаниеТиповЧисло17_2);

СтруктураГруппы3Стр2_1 = Новый ТаблицаЗначений;
СтруктураГруппы3Стр2_1.Колонки.Добавить("П000030007001", ОписаниеТиповСтрока4);
СтруктураГруппы3Стр2_1.Колонки.Добавить("П000030007002", ОписаниеТиповСтрока254);
СтруктураГруппы3Стр2_1.Колонки.Добавить("П000030007004", ОписаниеТиповСтрока15);
СтруктураГруппы3Стр2_1.Колонки.Добавить("П000030007005", ОписаниеТиповЧисло17_2);
